name = "Tupac"
name +=  " Shakur"

print(name)

status_message = "Hey, we have {} people using the site right now"
print(status_message.format(7))
print("Hey, we have {} {} using the site right now".format(3, 'people'))

away_message = "Hey, {} is still alive, you know?".format(name)
print(away_message)